@section head {
    <link rel="stylesheet" href="/Content/select2.css" />
    <script type="text/javascript" src="/Scripts/select2.js"></script>
    <script type="text/javascript" src="/Scripts/app_drivethru.js"></script>
    <style type="text/css">
        #controls {
            margin: 0;
            border-left: 0;
            border-right: 0;
            border-radius: 0;
        }
    </style>
}

<div class="well" id="controls">
    <div class="row-fluid location">
        <div class="span1 location-label">
            <label>Origin</label>
        </div>
        <div class="span1 location-button">
            <a class="btn" href="#"><i class="icon-map-marker"></i></a>
        </div>
        <div class="span10">
            <input type="hidden" id="from" style="width:100%" />
        </div>
    </div>
    <div class="row-fluid location">
        <div class="span1 location-label">
            <label>Destination</label>
        </div>
        <div class="span1 location-button">
            <a class="btn" href="#"><i class="icon-map-marker"></i></a>
        </div>
        <div class="span10">
            <input type="hidden" id="to" style="width:100%" />
        </div>
    </div>

</div>

<div id="map"></div>